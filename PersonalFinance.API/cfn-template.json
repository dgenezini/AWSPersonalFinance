{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Resources for PersonalFinance.API",
    "Resources": {
        "SourceBucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "AccessControl": "BucketOwnerFullControl"
            }
        }
    },
    "Outputs": {
      "BucketName": {
        "Description": "Bucket name",
        "Value": { 
            "Ref" : "SourceBucket" 
        }
      }
    }
}